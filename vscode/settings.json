{
    "window.zoomLevel": 0,
    "cmake.configureOnOpen": true,
    "keyboard.dispatch": "keyCode",
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "clang-format.executable": "/usr/bin/clang-format-7",
    "editor.minimap.enabled": false,
    "clang.executable": "/usr/bin/clang-9",
    "clang.completion.completeMacros": false,
    "clang.completion.enable": false,
    "catch2TestExplorer.userId": "267e48ebd8c73760e58674f549d31695e7e4959c",
    "C_Cpp.updateChannel": "Insiders",
    "C_Cpp.autocomplete": "Disabled",
    "C_Cpp.formatting": "Disabled",
    "C_Cpp.errorSquiggles": "Disabled",
    "C_Cpp.intelliSenseEngine": "Disabled",
    "clangd.path": "/home/patrykosmaczko/Apps/clangd_10rc3/bin/clangd",
    "vim.foldfix": true,
    "vim.camelCaseMotion.enable": true,
    "vim.leader": " ",
    "vim.useSystemClipboard": true,
    "vim.easymotion": true,
    "vim.highlightedyank.enable": true,
    "vim.handleKeys": {
        "<C-f>": false,
        "<C-k>": false,
        "<C-b>": false,
        "<C-j>": false,
        "<C-t>": false,
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "tab"
            ],
            "after": [
                "leader",
                "leader",
                "s"
            ]
        },
        {
            "before": [
                ";"
            ],
            "after": [
                ":"
            ]
        },
        {
            "before": [
                "g",
                "D"
            ], // go definition at side
            "commands": [
                "editor.action.revealDefinitionAside"
            ]
        },
        {
            "before": [
                "g",
                "u"
            ], // go usages (references)
            "commands": [
                "editor.action.goToReferences"
            ]
        },
        {
            "before": [
                " ",
                "r"
            ],
            "commands": [
                "build&run"
            ],
        },
        {
            "before": [
                " ",
                "b"
            ],
            "commands": [
                "build&tests"
            ],
        }
    ],
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "vim.easymotionMarkerHeight": 18,
    "vim.easymotionMarkerFontSize": "16",
    "vim.easymotionKeys": "hklyuiopnmqwertzxcvbasdgjf",
    "vim.replaceWithRegister": true,
    "clangd.arguments": [
        "-cross-file-rename",
        "-header-insertion=never"
    ],
    "catch2TestExplorer.logSentry": "disable_3",
    "gitlens.codeLens.enabled": false,
    "gitlens.advanced.repositorySearchDepth": 2,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "workbench.colorTheme": "Default Light+",
    "bracket-pair-colorizer-2.colors": [
        "Black",
        "Orchid",
        "LightSkyBlue"
    ],
    "bracket-pair-colorizer-2.forceUniqueOpeningColor": true,
    "cmake.buildBeforeRun": false,
}